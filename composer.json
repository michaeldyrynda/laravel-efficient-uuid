{
  "name": "dyrynda/laravel-efficient-uuid",
  "type": "utility",
  "description": "A package to extend Laravel migrations adding a more efficient storage of UUID fields in your database",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au",
      "homepage": "https://dyrynda.com.au"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/container": "^8.12",
    "illuminate/contracts": "^8.12",
    "illuminate/database": "^8.12",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "dyrynda/laravel-model-uuid": "^6.5",
    "laravel/legacy-factories": "^1.1",
    "mockery/mockery": "^1.4.2",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Dyrynda\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
