{
  "name": "dyrynda/laravel-efficient-uuid",
  "description": "A package to override Laravel migrations to more efficiently store UUID fields in your database",
  "type": "utility",
  "require": {
    "php": ">=5.6.4",
    "illuminate/container": "~5.4",
    "illuminate/database": "~5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "~0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Dyrynda\\Database\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au",
      "homepage": "https://dyrynda.com.au"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
        "providers": [
            "Dyrynda\\Database\\LaravelEfficientUuidServiceProvider"
        ]
    }
  }
}
